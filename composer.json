{
    "name": "bonesmccoy/bongo-fixture-loader",
    "description" : "simple mongo fixture loader",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bonesmccoy/bones-component-mongo.git"
        }
    ],
    "require": {
        "php" : ">5.4",
        "ext-mongo" : "*",
        "symfony/yaml" : "*",
        "bonesmccoy/bones-component-mongo" : "dev-master"
    },
    "bin": ["bin/bongo-load"],
    "require-dev": {
        "phpunit/phpunit" : "4.8.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "": "src"
        }
    }
}
